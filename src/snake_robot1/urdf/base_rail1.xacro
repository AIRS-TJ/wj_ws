    <xacro:macro name="base1" params="parent *origin name:=base base_joint_type:=prismatic">
        <link
            name="${name}_link">
            <visual>
            <origin
                xyz="0 0 0"
                rpy="0 0 0" />
            <geometry>
                <mesh
                filename="package://snake_robot1/meshes/base_link.STL" />
            </geometry>
            <material
                name="">
                <color
                rgba="0.75294 0.75294 0.75294 1" />
            </material>
            </visual>
            <collision>
            <origin
                xyz="0 0 0"
                rpy="0 0 0" />
            <geometry>
                <mesh
                filename="package://snake_robot1/meshes/base_link.STL" />
            </geometry>
            </collision>
        </link>
        <joint
        name="${name}_joint"
        type="${base_joint_type}">
        <xacro:insert_block name="origin" />
        <parent
            link="${parent}" />
        <child
            link="${name}_link" />
        <axis
            xyz="0 0 1" />
        <limit
        lower="-0.5"
        upper="0.5"
        effort="0"
        velocity="0" />
        </joint>
    </xacro:macro>

